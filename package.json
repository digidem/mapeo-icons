{
  "name": "mapeo-icons",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt preview",
    "generate": "nuxt generate",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "lint:js": "eslint --ext \".js,.ts,.vue\" .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@caohenghu/vue-colorpicker": "^1.2.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^10.1.2",
    "@nuxtjs/pwa": "^3.1.2",
    "bing-translate-api": "^4.1.0",
    "core-js": "^3.46.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mini-svg-data-uri": "^1.4.4",
    "nuxt": "^4.2.0",
    "oauth": "^0.10.2",
    "potrace": "^2.1.1",
    "svgo": "^4.0.0",
    "vue": "^3.5.22",
    "vue-color": "^3.3.3",
    "vue-server-renderer": "^2.2.6",
    "vue-template-compiler": "^0.1.0",
    "webpack": "^5.102.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/dotenv": "^1.4.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/stylelint-module": "^5.2.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@playwright/test": "^1.56.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1"
  }
